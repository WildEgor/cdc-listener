{
  "plugins": {
    "commit-analyzer": {
      "name": "default@^1.0.0"
    },
    "ci-condition": {
      "name": "default"
    },
    "changelog-generator": {
      "name": "default",
      "options": {
        "emojis": "false"
      }
    },
    "provider": {
      "name": "github",
      "options": {
        "slug": "wildegor/cdc-listener"
      }
    },
    "files-updater": {
      "names": ["npm"]
    }
  }
}